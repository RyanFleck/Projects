\def\GetInfo#1{%
  \def\filename{#1}%
  {\def\ver##1(##2)##3\relax{%
     \def\tc{##1}%
     \def\td{##2}%
     \ifx \td\empty %
     \else %
      \edef\td{$\csname ##2\endcsname$}%
     \fi}%
   \def\tb##1 v##2 ##3\end{%
   \gdef\filedate{##1}%
     \ver ##2()\relax%
     \xdef\fileversion{\tc\expandafter\noexpand\td}%
    \gdef\fileinfo{##3}}%
  \edef\ta{\csname ver@#1\endcsname}%
  \expandafter\tb\ta\end\relax}}

